num = int(input("enter an integer "))

# divisible by 2 and 5,
# divisible by 2 or 5,
# not divisible by 2 or 5

if num % 2 == 0 and num % 5 == 0:
    print("The integral is divisible by 2 and 5")
    
elif num % 2 == 0 or num % 5 == 0:
    print("The integral is divisible by 2 or 5")
    
elif num % 2 != 0 or num % 5 != 0:
    print("The integral is not divisible by 2 or 5")
